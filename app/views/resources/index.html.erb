<table class="table table-striped">
  <tr>
    <th>Title</th>
    <th>URL</th>
    <th>File</th>
    <th>View-count</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @resources.each do |resource| %>
  <tr>
    <td><%= resource.resource_title %></td>
    <td><%= link_to resource.resource_url %></td>
    <td><%= resource.resource_file %></td>
    <td><%= resource.resource_count %></td>
    <td><%= link_to 'Show', resource %></td>
    <td><%= link_to 'Edit', edit_resource_path(resource) %></td>
    <td><%= link_to 'Destroy', resource, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<div class="actions">
  <a class="btn btn-primary" <%= link_to 'New Resource', new_resource_path %> </a>
</div>


