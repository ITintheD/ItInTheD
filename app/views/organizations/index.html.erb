<h2>                  </h2>
<h1>Listing Organizations</h1>   
 

<table  class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Year Established</th>
    <th>Phone</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>URL</th>
    <th>Type</th>
    <th>Description</th>
    <th></th>
  </tr>

<% @organizations.each do |organization| %>
  <tr>
    <td><%= organization.org_state %></td>
    <td><%= organization.org_type %></td>
    <td><%= organization.city %></td>
    <td><%= organization.zip %></td>
    <td><%= organization.lat %></td>
    <td><%= organization.long %></td>
    <td><%= organization.org_url %></td>
    <td><%= organization.web_link %></td>
    <td><%= link_to 'Show', organization %></td>
    <td><%= link_to 'Edit', edit_organization_path(organization) %></td>
    <td><%= link_to 'Destroy', organization, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<a class="btn btn-primary btn-large" <%= link_to 'New Organization', new_organization_path %> </a>


