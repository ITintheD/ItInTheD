<div id="resources_container">
<h3>Sponsors</h3>  
 

<table  class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Year Established</th>
    <th>Phone</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>URL</th>
    <th>Type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @organizations.each do |organization| %>
  <tr>
    <td><%= organization.name %></td>
    <td><%= organization.org_start_year %></td>
    <td><%= organization.org_phone %></td>
    <td><%= organization.address %></td>
    <td><%= organization.city %></td>
    <td><%= organization.org_state %></td>
    <td><%= organization.zip %></td>
    <td><%= link_to organization.org_url, organization.org_url %></td>
    <td><%= organization.keyword_list %></td>
    <td id="showsponsor"><%= link_to 'Show', organization %></td>
    <td id="editsponsor"><%= link_to 'Edit', edit_organization_path(organization) %></td>
    <td id="destroysponsor"><%= link_to 'Destroy', organization, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<div class="actions">
    <a class="btn btn-primary" <%= link_to 'New Sponsor', new_organization_path %> </a>
</div>
