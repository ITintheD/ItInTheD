<div class="content_title"><p>Socials (optional)</p></div>
  <div class="myboxinner">
    <div class="row-fluid" id="extraspace">
      <div class="span4">
        <div class="control-group">
          <label class="left">
          <%=f.label "Facebook Profile URL" %>
          </label>
            <div class="controls">
            <%= f.text_field :soc_url, :value => "http://facebook.com/" %>
            </div>
        </div>
      </div>
      <div class="span4">
        <div class="control-group">
          <label class="left">
          <%=f.label "Twitter Profile URL" %>
           </label>

            <div class="controls">
            <%= f.url_field :soc_url, :value => "http://twitter.com/" %>
            </div>
        </div>
      </div>
      <div class="span4">
        <div class="control-group">
          <label class="left">
          <%=f.label "Google+ Profile URL" %>
          </label>

            <div class="controls">
            <%= f.text_field :soc_url, :value => "https://plus.google.com"  %>
            </div>
        </div>
      </div>
    </div> 

   <div class="row-fluid" id="extraspace">
      <div class="span4">
        <div class="control-group">
          <label class="left">
          <%=f.label "LinkedIn Profile URL" %>
          </label>

            <div class="controls">
             <%= f.text_field :soc_url, :value => "http://linkedin.com/" %>
            </div>
        </div>
      </div>
      <div class="span4">
        <div class="control-group">
          <label class="left">
         <%=f.label "MySpace Profile URL" %>
          </label>

            <div class="controls">
            <%= f.text_field :soc_url, :value => "http://myspace.com/" %>
            </div>
        </div>
      </div>
      <div class="span4">
        <div class="control-group">
        <label class="left">
        <%= f.label "Other Profile URL" %>
        </label>
          <div class="controls">
            <%= f.text_field :soc_url, :value => "http://" %>
            </div>
        </div>
      </div>
    </div> 
</div>
